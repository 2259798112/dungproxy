<?xml version="1.0" encoding="UTF-8"?>
<fetch verson="1.0">
	<property name="res" xpath="//tbody/tr" fetch="true">
		<property name="ip" xpath="/td[1]/script/text()"
			regex="Base64\.decode\(&quot;(.*)&quot;\)" decoder="base64" />
		<property name="port" xpath="/td[2]/span/script/text()"
			regex="Base64\.decode\(&quot;(.*)&quot;\)" decoder="base64" />

		<test group="type">
			<property name="typecheck" check="true" xpath="/td[3]/small/text()"
				regex="(HTTPS)"></property>
			<property name="type" value="2"></property>
		</test>
		<test group="type">
			<property name="typecheck" check="true" xpath="/td[3]/small/text()"
				regex=".*(socket).*"></property>
			<property name="type" value="3"></property>
		</test>
		<test group="type">
			<property name="typecheck" check="true" xpath="/td[3]/small/text()"
				regex=".*(vpn).*"></property>
			<property name="type" value="4"></property>
		</test>
		<test group="type">
			<property name="typecheck" check="true" xpath="/td[3]/small/text()"
				regex="(HTTP)"></property>
			<property name="type" value="1"></property>
		</test>
		<property name="address" xpath="/td[4]/a/text()"></property>
	</property>
</fetch>