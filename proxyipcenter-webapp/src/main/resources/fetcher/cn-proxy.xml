<?xml version="1.0" encoding="UTF-8"?>
<fetch verson="1.0">
	<property name="res" xpath="//tbody/tr" fetch="true">
		<property name="ip" xpath="/td[1]/text()"  regex="(\d+\.\d+\.\d+\.\d+)" require="true"></property>
		<property name="port" xpath="/td[2]/text()"  regex="(\d+)" require="true"></property>
		<property name="address" xpath="/td[3]/text()"></property>
		<property name="type" value="1"></property>
		<property name="speed" xpath="/td[4]/div/strong[@class='bar']/@style" regex="width。*(\d*)%。*"></property>
	</property>
</fetch>